{
  "name": "68000 Opcodes",
  "injectionSelector": "L:source",
  "patterns": [
      { "include": "#mnemonics" }
  ],
  "repository": {
    "mnemonics": 
    {
       "patterns": [
        {
          "name": "keyword.mnemonic.coldfire.coldfireopcodes",
          "match": "\\b(?i)(fdmove|fmovem)(\\.d)?"
        },
        {
          "name": "keyword.mnemonic.coldfire.coldfireopcodes",
          "match": "\\b(?i)(fmove|fsmove)(\\.[bwlsd])?"
        },
        {
          "name": "keyword.mnemonic.68000.68000opcodes",
          "match": "\\b(?i)(unlk)\\b"
        },
        {
          "name": "keyword.mnemonic.68000.68000opcodes",
          "match": "\\b(?i)(moveq)(\\.[b])?\\b"
        },
        {
          "name": "keyword.mnemonic.68000.68000opcodes",
          "match": "\\b(?i)(link)(\\.[w])?\\b"
        },
        {
          "name": "keyword.mnemonic.68000.68000opcodes",
          "match": "\\b(?i)([pl]ea|mov3q|movclr|movem)(\\.[l])?\\b"
        },
        {
          "name": "keyword.mnemonic.68000.68000opcodes",
          "match": "\\b(?i)(mv[sz])(\\.[bw])?\\b"
        },
        {
          "name": "keyword.mnemonic.68000.68000opcodes",
          "match": "\\b(?i)()(\\.[bws])?\\b"
        },
        {
          "name": "keyword.mnemonic.68000.68000opcodes",
          "match": "\\b(?i)()(\\.[bl])?\\b"
        },
        {
          "name": "keyword.mnemonic.68000.68000opcodes",
          "match": "\\b(?i)(movea)(\\.[wl])?\\b"
        },
        {
          "name": "keyword.mnemonic.68000.68000opcodes",
          "match": "\\b(?i)(move)(\\.[bwl])?\\b"
        }
        ]
    }
  },
  "scopeName": "opcodes.coldfire"
}